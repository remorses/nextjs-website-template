{
    "extends": ["next"],
    "plugins": ["split-classnames"],

    "rules": {
        "react/no-children-prop": "off",
        // "prettier/prettier": "warn",
        "react/jsx-no-target-blank": "off",
        "react/display-name": "off",
        "@next/next/no-img-element": "off",
        "split-classnames/split-classnames": [
            "warn",
            {
                "functionName": "cs"
            }
        ]
    }
}
