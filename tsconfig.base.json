{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "allowJs": true,
        "moduleResolution": "Node",
        "lib": [
            "es2020",
            "es7",
            "es6",
            "ESNext"
            // "dom"
        ],
        "declaration": true,
        "downlevelIteration": true,
        "declarationMap": true,
        "strict": false,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "jsx": "react",
        "skipLibCheck": true
    }
}
